<%- include('../partials/header'); -%>
<form method="POST" action="http://localhost:4000/event/edit/<%= itemsreturned[0].id %>" class="col-12">
  
<div class=" mt-5 form-group row">

<div class="col-6">
      <input type="text" required value='<%= itemsreturned[0].name %>' name="name"  class="form-control">
</div>
</div>
<div class=" form-group row">
    
    <div class="col-6">
        <br>
          <input  value='<%= itemsreturned[0].start %>'  required type="date" name="start"  class="form-control">
    </div>
    </div>
    <div class=" form-group row">
        
        <div class="col-6">
            <br>
              <input type="date" value='<%= itemsreturned[0].stop %>' required  name="stop"  class="form-control">
        </div>
        </div>
        <div class=" form-group row">
            
            <div class="col-6 mx-0">
                <br>
                  <input type="text" required value='<%=itemsreturned[0].location %>' name="location"  class="form-control">
            </div>
            <div  class='col-5 mt-3'>
                <input type="submit" required value="SAVE EVENT" class="btn btn-primary btn-lg">

            </div>
            </div>
</form>
<%- include('../partials/footer');  -%>